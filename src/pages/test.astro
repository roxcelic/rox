<script>
    // Example: Making a GET request to your Cloudflare Worker API
async function fetchDataFromWorker() {
  try {
    const response = await fetch('https://testworker.roxie.workers.dev/api/endpoint');
    if (response.ok) {
      const data = await response.json();
      console.log('Data received from Cloudflare Worker:', data);
      // Process the data as needed
    } else {
      console.error('Error fetching data from Cloudflare Worker:', response.status);
    }
  } catch (error) {
    console.error('An error occurred while fetching data:', error);
  }
}

// Call the function to fetch data
fetchDataFromWorker();

</script>